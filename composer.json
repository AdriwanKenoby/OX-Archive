{
    "name": "OX/Archivage",
    "description": "Archivage appclication for Mediboard",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "silex/silex": "^2.2",
        "silex/web-profiler": "^2.0",
        "symfony/asset": "^3.3",
        "symfony/class-loader": "^3.3",
        "symfony/config": "^3.3",
        "symfony/console": "^3.3",
        "symfony/debug": "^3.3",
        "symfony/finder": "^3.3",
        "symfony/form": "^3.3",
        "symfony/monolog-bridge": "^3.3",
        "symfony/process": "^3.3",
        "symfony/security": "^3.3",
        "symfony/translation": "^3.3",
        "symfony/twig-bridge": "^3.3",
        "symfony/validator": "^3.3",
        "doctrine/dbal": "^2.6",
        "twig/twig": "^2.4",
        "monolog/monolog": "^1.23",
        "twig/extensions": "^1.5",
        "guzzlehttp/guzzle": "^6.3",
        "elasticsearch/elasticsearch": "5.0",
